    <script>
        function searchOnEnter(event) {
            if (event.key === 'Enter') {
                search();
            }
        }

        function search() {
            var txtSearch = $("#search-main").val();
            $.ajax({
                type: "GET",
                url: "@Url.Action("Index", "Home")",
                data: {  search: txtSearch },
                success: function (result) {
                    if (result.success) {
                        window.location.href = '/Home?search='+txtSearch+'&reload=1';
                    } 
                },
                error: function () {
                    alert("Đã xảy ra lỗi khi gửi yêu cầu.");
                }
            });
        }
    </script>


    <script>
        // Lấy đường link URL hiện tại
        var currentURL = window.location.href;

        // Lấy tất cả các phần tử li trong menu
        var menuItems = document.querySelectorAll('.menu-item');

        // Duyệt qua từng phần tử và kiểm tra nếu href của nó trùng với đường link URL hiện tại
        menuItems.forEach(function (menuItem) {
            var menuItemLink = menuItem.querySelector('.menu-item-link');
            var menuItemURL = menuItemLink.getAttribute('href');

            // So sánh URL và thêm hoặc xóa class "active"
            if (currentURL.endsWith(menuItemURL)) {
                menuItem.classList.add('active');
            } else {
                menuItem.classList.remove('active');
            }
        });
    </script>

