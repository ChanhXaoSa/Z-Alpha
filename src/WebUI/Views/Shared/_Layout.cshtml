        * {
            box-sizing: border-box;
        }

 
        .search-box {
            width: fit-content;
            height: fit-content;
            position: absolute;
            margin-top:10px;
        }

        .input-search {
            height: 40px;
            width: 40px;
            border-style: none;
    
            font-size: 16px;
            outline: none;
            border-radius: 25px;
            transition: all .5s ease-in-out;
            background-color: #008CA1;
            padding-right: 40px;
            color: black;
        }

            .input-search::placeholder {
                color: wheat;
                font-size: 16px;
                font-weight: 100;
            }

        .btn-search {
            width: 40px;
            height: 40px;
            border-style: none;
            font-size: 16px;
            font-weight: bold;
            outline: none;
            cursor: pointer;
            border-radius: 50%;
            position: absolute;
            right: 0px;
            color: wheat;
            background-color: transparent;
            pointer-events: painted;
        }

            .btn-search:focus ~ .input-search {
                width: 22vw;
                border-radius: 0px;
                background-color: transparent;
                border-bottom: 1px solid black;
                transition: all 500ms cubic-bezier(0, 0.110, 0.35, 2);  
            }

        .input-search:focus {
            width: 22vw;
            border-radius: 0px;
            background-color: transparent;
            border-bottom: 1px solid black;
            transition: all 500ms cubic-bezier(0, 0.110, 0.35, 2);
        }

        .boxtext {
