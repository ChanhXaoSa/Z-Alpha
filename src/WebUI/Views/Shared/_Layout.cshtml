            <div class="user-avatar-content">
                <!-- HEXAGON -->
                @if (User.Identity.IsAuthenticated)
                {
                    if (userLogined !=null && userLogined.AvatarUrl?.Length > 0)
                    {
                        <div class="hexagon-image-30-32" data-src="@userLogined.AvatarUrl"></div>
                    }
                    else
                    {
                        <div class="hexagon-image-30-32" data-src="https://cdn1.iconfinder.com/data/icons/user-pictures/100/unknown-512.png"></div>
                    }
                }
