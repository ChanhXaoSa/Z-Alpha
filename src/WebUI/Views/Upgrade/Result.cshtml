@{
    ViewBag.Title = "Result Page";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container mt-5">
    <div class="card">
        <div class="card-header">
            Kết quả thanh toán
        </div>
        <div class="card-body">
            <h5 class="card-title">@ViewBag.ResultText</h5>
            <p class="card-text">
                <strong>@ViewBag.displayTmnCode</strong><br />
                <strong>@ViewBag.displayTxnRef</strong><br />
                <strong>@ViewBag.displayVnpayTranNo</strong><br />
                <strong>@ViewBag.displayAmount</strong><br />
                <strong>@ViewBag.displayBankCode</strong><br />
            </p>
            <a href="/" class="btn btn-primary">Quay lại trang chủ</a>
        </div>
    </div>
</div>
